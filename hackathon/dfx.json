{
  "canisters": {
    "hackathon_backend": {
      "main": "src/hackathon_backend/main.mo",
      "type": "motoko",
      "extra": {
        "dfx": {
          "canister_http": true
        }
      }
    },
    "hackathon_frontend": {
      "dependencies": [
        "hackathon_backend"
